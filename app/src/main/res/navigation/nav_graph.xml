    <fragment
        android:id="@+id/navigationCategory"
        android:name="com.sunasterisk.movie19.ui.category.view.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/navigationFavorite"
        android:name="com.sunasterisk.movie19.ui.favorite.FavoriteFragment"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" />
